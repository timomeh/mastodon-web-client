{
  "name": "mastodon-web-client",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "test": "react-app-rewired test --env=jsdom",
    "test:ci": "react-app-rewired test --env=jsdom --runInBand --reporters=default --reporters=jest-junit",
    "lint": "eslint .",
    "lint:ci": "eslint -f junit -o reports/eslint/results.xml .",
    "analyze": "source-map-explorer build/static/js/main.*",
    "analyze:ci": "mkdir -p analyze && source-map-explorer --html build/static/js/*  > analyze/source-map-explorer.html",
    "bundlesize": "bundlesize"
  },
  "dependencies": {
    "@manaflair/redux-batch": "0.1.0",
    "color-hash": "1.0.3",
    "emotion": "9.2.12",
    "emotion-theming": "9.2.9",
    "humps": "2.0.1",
    "is-valid-domain": "0.0.6",
    "normalizr": "3.2.4",
    "querystring": "0.2.0",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-emotion": "9.2.12",
    "react-lazyload": "2.3.0",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "react-scripts": "2.0.4",
    "recompose": "0.30.0",
    "redux": "4.0.0",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0",
    "webfontloader": "1.6.28"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "3.4.11",
    "@storybook/addon-actions": "3.4.11",
    "@storybook/addon-backgrounds": "3.4.11",
    "@storybook/addon-links": "3.4.11",
    "@storybook/addon-options": "3.4.11",
    "@storybook/addon-viewport": "3.4.11",
    "@storybook/addons": "3.4.11",
    "@storybook/react": "3.4.11",
    "babel-core": "6.26.3",
    "babel-plugin-emotion": "9.2.11",
    "babel-runtime": "6.26.0",
    "bundlesize": "0.17.0",
    "eslint": "5.6.1",
    "eslint-config-prettier": "3.1.0",
    "eslint-plugin-prettier": "2.7.0",
    "eslint-plugin-react": "7.11.1",
    "husky": "1.1.1",
    "jest-dom": "1.12.1",
    "jest-fetch-mock": "1.6.6",
    "jest-junit": "5.2.0",
    "lint-staged": "7.3.0",
    "prettier": "1.14.3",
    "react-app-rewired": "1.6.2",
    "react-testing-library": "5.2.0",
    "redux-devtools-extension": "2.13.5",
    "source-map-explorer": "1.6.0",
    "storybook-host": "5.0.1"
  },
  "bundlesize": [
    {
      "path": "./build/static/js/*.js"
    }
  ]
}
